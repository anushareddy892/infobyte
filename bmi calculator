print("Welcome To BMI Calculator Calculation")
weight=int(input("enter weight in kilograms:"))
height=float(input("enter height in meters:"))
bmi=weight/(height*height)
print(bmi)
if bmi<18.5:
    print("underweight")
elif bmi>=18.5 and bmi<25:
    print("Normal")
elif bmi>=25 and bmi<30:
    print("overweight")
else:
    print("obesity")
